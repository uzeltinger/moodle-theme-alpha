{{!
    Page footer.
}}

<footer>
    <div class="my-4 text-center">
                <div id="course-footer">{{{ output.course_footer }}}</div>

                <div class="rui-footer-btns">
                    {{# output.page_doc_link }}
                        <div class="helplink">{{{ output.page_doc_link }}}</div>
                    {{/ output.page_doc_link }}

                    {{# output.services_support_link }}
                        <div class="supportlink">{{{ output.services_support_link }}}</div>
                    {{/ output.services_support_link }}

                    {{# output.supportemail }}
                        <div class="suportemail">{{{ output.supportemail }}}</div>
                    {{/ output.supportemail }}

                    <div class="tool_usertours-resettourcontainer"></div>
                </div>
                <div class="rui-debug rui-footer-btns w-100 d-block my-2 small">{{{ output.debug_footer_html }}}{{#output.moodle_release}}<div class="mt-2 small">{{#str}}version, core{{/str}} {{{ output.moodle_release }}}</div>{{/output.moodle_release}}</div>
                <div class="rui-footer-btns">{{{ output.standard_footer_html }}}</div>
        </div>
</footer>
